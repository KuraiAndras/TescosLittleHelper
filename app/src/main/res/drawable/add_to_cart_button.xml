<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape>
            <gradient
                android:angle="270"
                android:endColor="#00FF44"
                android:startColor="#00FF44"/>
            <stroke
                android:width="1dp"
                android:color="#141111"/>
            <corners
                android:radius="7dp"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <gradient
                android:angle="270"
                android:endColor="#949C9C"
                android:startColor="#949C9C"/>
            <stroke
                android:width="1dp"
                android:color="#141111"/>
            <corners
                android:radius="7dp"/>
        </shape>
    </item>
</selector>